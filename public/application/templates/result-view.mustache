<div class="page-title">
    {{title}}
</div>

<!-- COUNTER-->
<div id="departure-in" class="travel-info-counter">
    {{nextDepartureTime}}
</div>

<!-- FROM TO VIEW-->
<div class="small-buss-button-container">
    <button class="small-buss-button">{{from}}</button>
    -
    <button class="small-buss-button">{{to}}</button>
</div>

<!-- TRAVEL INFO TABLE -->
<div class="travel-info-container" id="travel-info-container">
    <table class="travel-info-table">
    {{#routes}}
        <tr class="travel-info-entry">
            <td>{{route}}</td>
            <td>{{departureTime}}</td>
            <!-- IF YOU WANT TO SHOW DEPARTURE TIME <td>{{arrivalTime}}</td>-->
            <td>{{travelTime}}</td>
        </tr>
    {{/routes}}
    </table>
</div>

<div class="page-button-container" id="page-button-container">
    <button id="new-search-button" class="page-button">Nytt s√∏k</button>
</div>